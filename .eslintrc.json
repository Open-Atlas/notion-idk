{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": [
            "error",
            180
        ],
        "no-tabs": 0,
        "require-jsdoc": 0,
        "no-invalid-this": 0
    }
}